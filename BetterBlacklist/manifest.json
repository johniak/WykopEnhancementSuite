{
    "manifest_version": 2,

    "name": "Better czarnolisto",
    "description": "Extension for wykop, which increase black list funcionality",
    "version": "0.1",

    "permissions": [
        "webNavigation",
        "https://www.wykop.pl/",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [ "https://www.wykop.pl/*", "http://www.wykop.pl/*" ],
            "js": [ "jquery-1.10.1.min.js", "blacklist.js" ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        "blacklist.js.map"
    ],
    "options_page": "options.html",
    "background": {
        "scripts": ["SettingManager.js", "background.js" ]
    }
}